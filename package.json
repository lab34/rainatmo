{
  "name": "rainatmo",
  "version": "1.0.0",
  "description": "Application web de visualisation des données de pluviométrie Netatmo",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "init-db": "node src/scripts/init-historical-data.js",
    "test": "node --test tests/**/*.test.js",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\" \"public/**/*.{html,css,js}\""
  },
  "keywords": ["netatmo", "weather", "rainfall", "pluviometry"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^4.26.0",
    "@fastify/static": "^7.0.1",
    "@fastify/cors": "^9.0.1",
    "sql.js": "^1.10.3",
    "node-cron": "^3.0.3",
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.4"
  }
}
